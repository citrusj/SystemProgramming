      "source": [
        "import numpy as np\n",
        "size = int(input())\n",
        "size2 = int(size/2)\n",
        "A = np.round(np.random.random((size,size)),2)\n",
        "B = np.zeros((size2,size2))\n",
        "for i in range(size2):\n",
        "    for j in range(size2):\n",
        "        B[i][j] = np.amax(A[i*2:i*2+2,j*2:j*2+2]) \n",
        "print(A)\n",
        "print(B)\n"
      ],
