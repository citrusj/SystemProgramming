      "source": [
        "num = int(input())\n",
        "A = list(int(i) for i in input().split())\n",
        "countL = [0] * num\n",
        "for i in A:\n",
        "    if i==num+1: countL = [max(countL)]*num\n",
        "    else: countL[i-1]+=1\n",
        "        \n",
        "for i in countL: print(i, end=\" \")"
